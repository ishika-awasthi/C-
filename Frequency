#include<iostream>
using namespace std;
int main(){
  int size=10;
  int a[100]= {1,2,3,1,4,5,2,5,1,3};
  const int max = 100;
  int frequency[max + 1] = {0};
  for (int i = 0; i < size; ++i) {
    frequency[a[i]]++;}
  cout << "Frequency of each element in the array:" << endl;
  for(int i= 0;i<=max; ++i) {
    if (frequency[i] > 0) {
  cout <<"Element"<< i <<": "<<frequency[i]<<" times"<<endl;
 }}
}
